{
  "name": "counter-test",
  "version": "1.0.0",
  "description": "MongoDB performance testing with fact generation",
  "main": "src/index.js",
  "scripts": {
    "test:logger": "set LOG_LEVEL=DEBUG && node src/tests/loggerTest.js",
    "test:generator": "set LOG_LEVEL=DEBUG && node src/tests/messageGeneratorTest.js",
    "test:mapper": "set LOG_LEVEL=DEBUG && node src/tests/factMapperTest.js",
    "test:indexer": "set LOG_LEVEL=DEBUG && node src/tests/factIndexerTest.js",
    "test:computation": "set LOG_LEVEL=DEBUG && node src/tests/computationConditionsTest.js",
    "test:counters": "set LOG_LEVEL=DEBUG && node src/tests/counterProducerTest.js",
    "test:mongo-index": "set LOG_LEVEL=DEBUG && node src/tests/mongoFactIndexTest.js",
    "test:mongo": "set LOG_LEVEL=DEBUG && node src/tests/mongoProviderTest.js",
    "test:controller": "set LOG_LEVEL=DEBUG && node src/tests/factControllerTest.js",
    "test:api": "set LOG_LEVEL=INFO && node src/tests/webTest.js",
    "start": "set LOG_LEVEL=INFO && node src/start.js",
    "start:service": "set LOG_LEVEL=INFO && node src/web/cluster.js",
    "start:worker": "node src/web/worker.js",
    "start:test": "set LOG_LEVEL=INFO && node src/serviceTest.js",
    "diagnose": "set LOG_LEVEL=INFO && node src/utils/runDiagnostics.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mongodb": "^6.20.0",
    "xml2js": "^0.6.2"
  },
  "keywords": [
    "mongodb",
    "performance",
    "testing",
    "web-service",
    "cluster"
  ],
  "author": "",
  "license": "MIT",
  "pkg": {
    "targets": [
      "node18-linux-x64"
    ],
    "outputPath": "dist",
    "assets": [
      "countersConfig.json",
      "indexConfig.json",
      "messageConfig.json"
    ],
    "scripts": [
      "src/**/*.js"
    ]
  },
  "bin": "src/web/cluster.js"
}
