    "evaluationConditions": {
      "$expr": {
        "$gte": [
          "$d.dt",
          {
            "$dateAdd": {
              "startDate": "$$NOW",
              "unit": "day",
              "amount": -700
            }
          }
        ]
      }
    },
    "attributes": {
      "cnt": {
        "$sum": 1
      },
      "sum_amount": {
        "$sum": "$d.a"
      },
      "count_distinct": {
